{
  "name": "epsicube/module-execution-platform",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "COLANT Alan",
      "email": "contact@alancolant.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "illuminate/support": "^12.8",
    "illuminate/database": "^12.8",
    "epsicube/support": "*",
    "phiki/phiki": "^2.0.4",
    "codeat3/blade-clarity-icons": "^1.8"
  },
  "suggest": {
    "epsicube/module-administration": "Executions can be tracked in administration"
  },
  "autoload": {
    "psr-4": {
      "EpsicubeModules\\ExecutionPlatform\\": "."
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "epsicube": {
      "modules": [
        "EpsicubeModules\\ExecutionPlatform\\ExecutionPlatformModule"
      ]
    }
  }
}
